{
	"env": {
		"jest": true,
		"browser": true,
		"es2021": true
	},
	"extends": [
		"next/core-web-vitals",
		"plugin:react/recommended",
		"standard",
		"plugin:react/jsx-runtime",
		"eslint-config-prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react-hooks/exhaustive-deps": "off"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
